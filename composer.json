{
    "name": "shamithewebdeveloper/laravel-nytimes-api",
    "description": "A Laravel package for working with the New York Times API.",
    "keywords": [
        "laravel",
        "nytimes",
        "news",
        "api",
        "package"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ehtisham Asghar",
            "email": "ehtishamasghar227@gmail.com"
        }
    ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
        "psr-4": {
            "ShamiTheWebdeveloper\\NYTimes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShamiTheWebdeveloper\\NYTimes\\NYTimesServiceProvider"
            ],
            "aliases": {
                "NYTimes": "ShamiTheWebdeveloper\\NYTimes\\Facades\\NYTimes"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
